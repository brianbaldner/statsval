<Window
    x:Class="uselessapp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:gif="http://wpfanimatedgif.codeplex.com"
    xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="848.01"
    Height="491.714"
    Background="#181735"
    Closing="Window_Closing"
    ResizeMode="NoResize"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.Resources>
        <ControlTemplate x:Key="MatchData" TargetType="Button">
            <Border
                Grid.Row="0"
                Height="65"
                Margin="5,0,5,0"
                VerticalAlignment="Top"
                Background="{TemplateBinding Background}"
                BorderBrush="#181A20"
                BorderThickness="3">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="15*" />
                        <ColumnDefinition Width="12*" />
                        <ColumnDefinition Width="19*" />
                        <ColumnDefinition Width="15*" />
                        <ColumnDefinition Width="15*" />
                    </Grid.ColumnDefinitions>
                    <StackPanel Grid.Column="0">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            FontSize="15"
                            FontWeight="Medium"
                            Foreground="LightGray"
                            Text="{TemplateBinding ContentStringFormat}" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="25"
                            FontWeight="Medium"
                            Foreground="White"
                            Text="{TemplateBinding Name}" />
                    </StackPanel>
                    <StackPanel Grid.Column="1">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            FontSize="15"
                            FontWeight="Medium"
                            Foreground="LightGray"
                            Text="Score" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="25"
                            FontWeight="Medium"
                            Foreground="White"
                            Text="{TemplateBinding Tag}" />
                    </StackPanel>
                    <StackPanel Grid.Column="2">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            FontSize="15"
                            FontWeight="Medium"
                            Foreground="LightGray"
                            Text="K/D/A" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="25"
                            FontWeight="Medium"
                            Foreground="White"
                            Text="{TemplateBinding CommandParameter}" />
                    </StackPanel>
                    <StackPanel Grid.Column="3">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            FontSize="15"
                            FontWeight="Medium"
                            Foreground="LightGray"
                            Text="KD" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="25"
                            FontWeight="Medium"
                            Foreground="White"
                            Text="{TemplateBinding Content}" />
                    </StackPanel>
                    <StackPanel Grid.Column="5">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            FontSize="15"
                            FontWeight="Medium"
                            Foreground="LightGray"
                            Text="Score" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="25"
                            FontWeight="Medium"
                            Foreground="White"
                            Text="{TemplateBinding Uid}" />
                    </StackPanel>
                </Grid>
            </Border>
        </ControlTemplate>
    </Window.Resources>
